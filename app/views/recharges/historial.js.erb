$('#table_historial tbody').append('<%= j render "recharges_from_historial", recharges: @recharges%>')

<% if @recharges.next_page%>
    $('.pagination').replaceWith('<%= j will_paginate @recharges, class:"pagination d-none"%>')
<%else%>
    $('.pagination').remove()
    $('.contenedor_loader').remove()
<%end%>